{
    "name": "basketin/laravel-paymob",
    "description": "Dealing with payments through the Egyptian payment gateway PayMob",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "ecommerce",
        "paymob",
        "payments",
        "baskets"
    ],
    "authors": [
        {
            "name": "Karim Mohamed (Komtcho)",
            "email": "komicho1996@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1||^8.2",
        "laravel/framework": "^10.0||^11.0",
        "komtcho/shot": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Basketin\\Paymob\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Basketin\\Paymob\\PaymobServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}